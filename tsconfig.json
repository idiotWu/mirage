{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "es6"
    ],
    "moduleResolution": "node",
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noImplicitThis": true,
    "noUnusedLocals": true,
    "importHelpers": true,
    "sourceMap": true,
  },
  "exclude": [
    "node_modules",
    "dist"
  ]
}
